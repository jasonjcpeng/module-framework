{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "declaration": true,
        "declarationDir": "dist",
        "typeRoots": ["./types", "./node_modules/@types"],
        "skipLibCheck": true,
        "moduleResolution": "Node",
        "lib": ["ES2015", "DOM"]
    }
}
